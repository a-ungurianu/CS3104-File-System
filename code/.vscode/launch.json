    "version": "0.2.0",
    "configurations": [

    {
        "name": "GDB: Home",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/myfs",
        "args": ["-s","-d","test"],
        "stopAtEntry": false,
        "cwd": "${workspaceFolder}",
        "environment": [],
        "externalConsole": true,
        "MIMode": "gdb",
        "setupCommands": [
            {
                "description": "Enable pretty-printing for gdb",
                "text": "-enable-pretty-printing",
                "ignoreFailures": true
            }
        ]
    },
    {
        "name": "GDB: Lab",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/myfs",
        "args": ["-s","-d","/cs/scratch/iau2/mnt2"],
        "stopAtEntry": false,
        "cwd": "${workspaceFolder}",
        "environment": [],
        "externalConsole": true,
        "MIMode": "gdb",
        "setupCommands": [
            {
                "description": "Enable pretty-printing for gdb",
                "text": "-enable-pretty-printing",
                "ignoreFailures": true
            }
        ]
    }
    ]
}